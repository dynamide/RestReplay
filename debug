pushd ~/src/RestReplay

mvn -o -DskipTests=true install

java -jar target/RestReplay-1.0.4-standalone.jar \
   -master _self_test/master-self-test.xml \
   -control _self_test/self-test.xml \
   -testGroup debug  \
   -testdir ./src/main/resources/restreplay \
   -selftest \
   -reports /Users/vcrocla/src/RestReplay/rest-replay-reports
popd
