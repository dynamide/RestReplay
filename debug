pushd ~/src/RestReplay

. ./java-options

mvn -q -o -DskipTests=true install

java $JAVA_OPTS -jar target/$RESTREPLAY_JAR \
   -master _self_test/master-self-test.xml \
   -control _self_test/self-test.xml \
   -testGroup debug  \
   -testdir ./src/main/resources/restreplay \
   -selftest \
   -reports /Users/vcrocla/src/RestReplay/reports
popd
