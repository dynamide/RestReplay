<?xml version="1.0" encoding="UTF-8"?>
<restReplayMaster>
    <protoHostPort>http://localhost:8180</protoHostPort>
    
    <!-- legal values for dumpServiceResult=[minimal,detailed,full,auto] -->
    <dump payloads="false" dumpServiceResult="auto" />

    <runOptions>
        <connectionTimeout>3000</connectionTimeout>
        <socketTimeout>3000</socketTimeout>
        <errorsBecomeEmptyStrings>true</errorsBecomeEmptyStrings>
    </runOptions>

    <vars>
        <var ID="DMSERVER">http://localhost:18080</var>
    </vars>

    <run controlFile="_self_test/dynamide.xml" testGroup="login" />

    <run controlFile="_self_test/dynamide.xml" testGroup="main" />


</restReplayMaster>

